  /**
   * æœç´¢æ–‡ä»¶å¹¶è¿”å›è¯¦ç»†ä¿¡æ¯
   */
  async searchFilesWithInfo(keyword: string): Promise<FileInfo[]> {
    try {
      const allFiles = await this.getAllFilesWithInfo();

      if (!keyword || keyword.trim() === '') {
        return allFiles;
      }

      const searchTerm = keyword.toLowerCase();
      return allFiles.filter(fileInfo =>
      fileInfo.filename.toLowerCase().includes(searchTerm)
      );
    } catch (error) {
      console.error(`æœç´¢æ–‡ä»¶å¤±è´¥: ${error.message}`);
      return [];
    }
  }
  /**
   * è·å–å•ä¸ªæ–‡ä»¶çš„è¯¦ç»†ä¿¡æ¯
   */
  async getFileInfo(filename: string): Promise<FileInfo | null> {
    try {
      const filePath = this.filesDir + '/' + filename;

      // æ£€æŸ¥æ–‡ä»¶æ˜¯å¦å­˜åœ¨
      try {
        fileIo.accessSync(filePath);
      } catch (e) {
        console.log(`æ–‡ä»¶ä¸å­˜åœ¨: ${filename}`);
        return null;
      }

      // è·å–æ–‡ä»¶çŠ¶æ€ä¿¡æ¯
      const stat = fileIo.statSync(filePath);

      // æ ¼å¼åŒ–æ–‡ä»¶å¤§å°
      const sizeReadable = this.formatFileSize(stat.size);

      // æ ¼å¼åŒ–æ—¶é—´
      const mtimeFormatted = this.formatDate(stat.mtime);
      const ctimeFormatted = this.formatDate(stat.ctime);

      return {
        filename: filename,
        size: stat.size,
        sizeReadable: sizeReadable,
        mtime: stat.mtime,
        ctime: stat.ctime,
        mtimeFormatted: mtimeFormatted,
        ctimeFormatted: ctimeFormatted
      };
    } catch (error) {
      console.error(`è·å–æ–‡ä»¶ä¿¡æ¯å¤±è´¥: ${error.message}`);
      return null;
    }
  }

  // ä¿®æ”¹ getAllFilesWithInfo æ–¹æ³•
  async getAllFilesWithInfo(): Promise<FileInfo[]> {
    try {
      const filesDir = this.getFilesDir();
      console.log(`[getAllFilesWithInfo] è¯»å–ç›®å½•: ${filesDir}`);

      // ä½¿ç”¨ fileIo è€Œä¸æ˜¯ fsï¼ˆä¸ getAllFiles ä¿æŒä¸€è‡´ï¼‰
      const files = fileIo.listFileSync(filesDir);
      console.log(`[getAllFilesWithInfo] æ‰¾åˆ°æ–‡ä»¶: ${files.length} ä¸ª`);

      const fileInfos: FileInfo[] = [];

      for (const file of files) {
        try {
          // æ„å»ºå®Œæ•´è·¯å¾„
          const filePath = `${filesDir}/${file}`;

          // è·å–æ–‡ä»¶ä¿¡æ¯
          const stat = fileIo.statSync(filePath);

          // è·³è¿‡ç›®å½•ï¼Œåªæ˜¾ç¤ºæ–‡ä»¶
          if (stat.isDirectory()) {
            console.log(`[getAllFilesWithInfo] è·³è¿‡ç›®å½•: ${file}`);
            continue;
          }

          // æ ¼å¼åŒ–ä¸ºå¯è¯»ä¿¡æ¯
          const sizeReadable = this.formatFileSize(stat.size);
          const mtimeFormatted = this.formatDate(stat.mtime);
          const ctimeFormatted = this.formatDate(stat.ctime);

          fileInfos.push({
            filename: file,
            size: stat.size,
            sizeReadable: sizeReadable,
            mtime: stat.mtime,
            ctime: stat.ctime,
            mtimeFormatted: mtimeFormatted,
            ctimeFormatted: ctimeFormatted
          });

        } catch (fileError) {
          console.warn(`[getAllFilesWithInfo] å¤„ç†æ–‡ä»¶ ${file} å¤±è´¥:`, fileError);
          // å³ä½¿è·å–ä¿¡æ¯å¤±è´¥ï¼Œä¹Ÿæ·»åŠ åŸºæœ¬æ–‡ä»¶å
          fileInfos.push({
            filename: file,
            size: 0,
            sizeReadable: '0 B',
            mtime: Date.now(),
            ctime: Date.now(),
            mtimeFormatted: 'æœªçŸ¥',
            ctimeFormatted: 'æœªçŸ¥'
          });
        }
      }

      console.log(`[getAllFilesWithInfo] è¿”å›æ–‡ä»¶ä¿¡æ¯: ${fileInfos.length} ä¸ª`);
      return fileInfos;

    } catch (error) {
      console.error(`[getAllFilesWithInfo] è·å–æ–‡ä»¶åˆ—è¡¨å¤±è´¥:`, error);
      return [];
    }
  }

  // ç¡®ä¿ formatFileSize å’Œ formatDate æ–¹æ³•æ­£ç¡®å®ç°
  private formatFileSize(bytes: number): string {
    if (bytes === 0) return '0 B';

    const k = 1024;
    const sizes = ['B', 'KB', 'MB', 'GB'];
    const i = Math.floor(Math.log(bytes) / Math.log(k));

    return `${parseFloat((bytes / Math.pow(k, i)).toFixed(1))} ${sizes[i]}`;
  }

  private formatDate(timestamp: number): string {
    try {
      const date = new Date(timestamp);
      const year = date.getFullYear();
      const month = String(date.getMonth() + 1).padStart(2, '0');
      const day = String(date.getDate()).padStart(2, '0');

      return `${year}-${month}-${day}`;
    } catch (error) {
      return 'æœªçŸ¥æ—¥æœŸ';
    }
  }
