  async renameFile(oldName: string, newName: string): Promise<boolean> {
    try {
      // 1. éªŒè¯å‚æ•°
      if (!oldName || !newName || oldName.trim() === '' || newName.trim() === '') {
        console.error('é‡å‘½åå¤±è´¥ï¼šæ–‡ä»¶åä¸èƒ½ä¸ºç©º');
        return false;
      }

      // 2. ä¿®å‰ªç©ºæ ¼
      oldName = oldName.trim();
      newName = newName.trim();

      // 3. æ£€æŸ¥æ–°æ—§æ–‡ä»¶åæ˜¯å¦ç›¸åŒ
      if (oldName === newName) {
        console.error('é‡å‘½åå¤±è´¥ï¼šæ–°æ—§æ–‡ä»¶åç›¸åŒ');
        return false;
      }

      // 4. éªŒè¯æ–°æ–‡ä»¶åæ˜¯å¦åˆæ³•
      if (!this.isValidFilename(newName)) {
        console.error(`é‡å‘½åå¤±è´¥ï¼šæ–°æ–‡ä»¶åä¸åˆæ³• ${newName}`);
        return false;
      }

      // 5. æ„å»ºå®Œæ•´è·¯å¾„
      const oldPath = `${this.filesDir}/${oldName}`;
      const newPath = `${this.filesDir}/${newName}`;

      console.log(`é‡å‘½åæ–‡ä»¶: ${oldPath} -> ${newPath}`);

      // 6. æ£€æŸ¥åŸæ–‡ä»¶æ˜¯å¦å­˜åœ¨
      if (!await this.fileExists(oldName)) {
        console.error(`é‡å‘½åå¤±è´¥ï¼šåŸæ–‡ä»¶ä¸å­˜åœ¨ ${oldName}`);
        return false;
      }

      // 7. æ£€æŸ¥æ–°æ–‡ä»¶åæ˜¯å¦å·²å­˜åœ¨
      if (await this.fileExists(newName)) {
        console.error(`é‡å‘½åå¤±è´¥ï¼šæ–°æ–‡ä»¶å·²å­˜åœ¨ ${newName}`);
        return false;
      }

      // 8. æ‰§è¡Œé‡å‘½å
      try {
        fileIo.renameSync(oldPath, newPath);
        console.log(`é‡å‘½åæˆåŠŸ: ${oldName} -> ${newName}`);
        return true;
      } catch (error) {
        console.error(`é‡å‘½åå¤±è´¥: ${error.message || error}`);
        return false;
      }

    } catch (error) {
      console.error(`é‡å‘½åæ–‡ä»¶æ—¶å‘ç”Ÿé”™è¯¯: ${error}`);
      return false;
    }
  }

  /**
   * éªŒè¯æ–‡ä»¶åæ˜¯å¦åˆæ³•
   * @param filename æ–‡ä»¶å
   * @returns æ˜¯å¦åˆæ³•
   */
  private isValidFilename(filename: string): boolean {
    if (!filename || filename.trim() === '') {
      return false;
    }

    // æ£€æŸ¥éæ³•å­—ç¬¦
    const illegalChars = /[<>:"/\\|?*]/;
    if (illegalChars.test(filename)) {
      return false;
    }

    // æ£€æŸ¥æ–‡ä»¶åé•¿åº¦
    if (filename.length > 255) {
      return false;
    }

    // æ£€æŸ¥ç³»ç»Ÿä¿ç•™åç§°
    const reservedNames = ['CON', 'PRN', 'AUX', 'NUL', 'COM1', 'COM2', 'COM3', 'COM4',
      'COM5', 'COM6', 'COM7', 'COM8', 'COM9', 'LPT1', 'LPT2',
      'LPT3', 'LPT4', 'LPT5', 'LPT6', 'LPT7', 'LPT8', 'LPT9'];
    const nameUpper = filename.split('.')[0].toUpperCase();
    if (reservedNames.includes(nameUpper)) {
      return false;
    }

    return true;
  }

  async updateFile(filename: string, content: string): Promise<boolean> {
    try {
      const filePath = this.getFilesDir() + '/' + filename;
      if (!await this.fileExists(filename)) {
        return false;
      }

      const file = await fs.open(filePath, fs.OpenMode.WRITE_ONLY | fs.OpenMode.TRUNC);
      const arrayBuffer = new ArrayBuffer(content.length * 2);
      const dataView = new DataView(arrayBuffer);

      for (let i = 0; i < content.length; i++) {
        dataView.setUint16(i * 2, content.charCodeAt(i), true);
      }

      await fs.write(file.fd, arrayBuffer);
      await fs.close(file);

      Logger.info(`File updated: ${filename}`);
      return true;
    } catch (error) {
      Logger.error(`updateFile failed: ${JSON.stringify(error)}`);
      return false;
    }
  }


